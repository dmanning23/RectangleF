<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>RectangleF</id>
		<version>4.0.0</version>
		<title>RectangleF</title>
		<authors>dmanning23</authors>
		<projectUrl>https://github.com/dmanning23/RectangleF</projectUrl>
		<licenseUrl>https://github.com/dmanning23/RectangleF/blob/master/LICENSE.txt</licenseUrl>
		<description>Simple MonoGame library for a rectangle that uses float values instead of int</description>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.7"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.7"/>
			</group>

			<group targetFramework="net452">
				<dependency id="MonoGame.Framework.WindowsDX" version="3.7"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<!--Xamarin.Android-->
		<file src="RectangleF.Android\bin\Release\RectangleF.Android.pdb" target="lib\MonoAndroid10\RectangleF.Android.pdb" />
		<file src="RectangleF.Android\bin\Release\RectangleF.Android.dll" target="lib\MonoAndroid10\RectangleF.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="RectangleF.iOS\bin\Release\RectangleF.iOS.pdb" target="lib\Xamarin.iOS10\RectangleF.iOS.pdb" />
		<file src="RectangleF.iOS\bin\Release\RectangleF.iOS.dll" target="lib\Xamarin.iOS10\RectangleF.iOS.dll" />

		<!--WindowsDX-->
		<file src="RectangleF.WindowsDX\bin\Release\RectangleF.WindowsDX.pdb" target="lib\net452\RectangleF.WindowsDX.pdb" />
		<file src="RectangleF.WindowsDX\bin\Release\RectangleF.WindowsDX.dll" target="lib\net452\RectangleF.WindowsDX.dll" />

	</files>
</package>
