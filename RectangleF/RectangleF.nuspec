<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
	<metadata>
		<id>RectangleF</id>
		<version>1.0.0</version>
		<title>RectangleF</title>
		<authors>dmanning23</authors>
		<owners>dmanning23</owners>
		<projectUrl>https://github.com/dmanning23/RectangleF</projectUrl>
		<licenseUrl>https://github.com/dmanning23/RectangleF/blob/master/LICENSE.txt</licenseUrl>
		<requireLicenseAcceptance>false</requireLicenseAcceptance>
		<description>Simple MonoGame library for a rectangle that uses float values instead of int</description>
		<releaseNotes />
		<copyright>Copyright 2016</copyright>
		<tags>pcl, windows phone, winphone, android, ios, windows universal, monogame</tags>
		<dependencies>

			<!--Xamarin.Android-->
			<group targetFramework="MonoAndroid10">
				<dependency id="MonoGame.Framework.Android" version="3.5"/>
			</group>

			<!--Xamarin.iOS Unified-->
			<group targetFramework="Xamarin.iOS10">
				<dependency id="MonoGame.Framework.iOS" version="3.5"/>
			</group>

			<!--Win Universal-->
			<group targetFramework="uap10.0">
				<dependency id="MonoGame.Framework.WindowsUniversal" version="3.5"/>
				<dependency id="Microsoft.NETCore.UniversalWindowsPlatform" version="5.1"/>
			</group>

			<group targetFramework="net451">
				<dependency id="MonoGame.Framework.DesktopGL" version="3.5"/>
			</group>

		</dependencies>
	</metadata>
	<files>

		<!--Xamarin.Android-->
		<file src="RectangleF.Android\bin\Release\RectangleF.Android.pdb" target="lib\MonoAndroid10\RectangleF.Android.pdb" />
		<file src="RectangleF.Android\bin\Release\RectangleF.Android.dll" target="lib\MonoAndroid10\RectangleF.Android.dll" />

		<!--Xamarin.iOS Unified-->
		<file src="RectangleF.iOS\bin\Release\RectangleF.iOS.pdb" target="lib\Xamarin.iOS10\RectangleF.iOS.pdb" />
		<file src="RectangleF.iOS\bin\Release\RectangleF.iOS.dll" target="lib\Xamarin.iOS10\RectangleF.iOS.dll" />

		<!--Windows Universal-->
		<file src="RectangleF.WindowsUniversal\bin\Release\RectangleF.WindowsUniversal.pdb" target="lib\uap10.0\RectangleF.WindowsUniversal.pdb" />
		<file src="RectangleF.WindowsUniversal\bin\Release\RectangleF.WindowsUniversal.dll" target="lib\uap10.0\RectangleF.WindowsUniversal.dll" />

		<!--DesktopGL-->
		<file src="RectangleF.DesktopGL\bin\Release\RectangleF.DesktopGL.pdb" target="lib\net451\RectangleF.DesktopGL.pdb" />
		<file src="RectangleF.DesktopGL\bin\Release\RectangleF.DesktopGL.dll" target="lib\net451\RectangleF.DesktopGL.dll" />

	</files>
</package>
